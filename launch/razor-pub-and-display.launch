<launch>
  <arg name="razor_config_file" default="$(find nautonomous_sensor_imu)/config/my_razor.yaml"/>
  <node pkg="nautonomous_sensor_imu" type="imu_node.py" name="imu_node" output="screen">
    <rosparam file="$(arg razor_config_file)" command="load"/>
  </node>
  <node pkg="nautonomous_sensor_imu" type="display_3D_visualization.py" name="display_3D_visualization_node" output="screen">
  </node>
</launch>
